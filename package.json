    "browserify": "^9.0.7",
    "jasmine": "^2.2.1",
    "jscs": "^1.12.0",
    "jshint": "^2.7.0",
    "minifyify": "^6.4.0",
    "mocha": "^2.2.4",
    "mocha-traceur": "",
    "mockery": "^1.4.0",
    "should": "^5.2.0",
    "sinon": "^1.14.1",
    "traceur": "0.0.89",
    "watch": "^0.16.0",
    "flow-bin": "^0.18.1"
  }
